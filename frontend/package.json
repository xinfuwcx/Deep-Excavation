    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@react-three/drei": "^9.88.13",
    "@react-three/fiber": "^8.15.19",
    "axios": "^1.10.0",
    "chroma-js": "^2.4.2",
    "color": "^4.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.3",
    "stats.js": "^0.17.0",
